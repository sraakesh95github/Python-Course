    "\n",
    "def fact(x) :\n",
    "    if x >= 2 :\n",
    "        return x * fact(x-1)\n",
    "    else :\n",
    "        return 1\n",
    "\n",
    "        \n",
    "x = int(input(\"Enter the number for which you want the factorial : \"))\n",
    "fact(x)"
